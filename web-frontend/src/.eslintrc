{
	"rules": {
		"indent": [
			2,
			"tab", {
				"SwitchCase": 1
			}
		],
		"quotes": [
			2,
			"single"
		],
		"linebreak-style": [
			2,
			"unix"
		],
		"semi": [
			2,
			"always"
		],
		"angular/di": [
			2,
			"array"
		],
		"no-mixed-spaces-and-tabs": [
			2, "smart-tabs"
		]
	},
	"env": {
		"es6": true,
		"browser": true
	},
	"extends": "angular"
}